<odoo>
    <data>
        <record id="view_warehouse_orderpoint_tree_editable_inherit" model="ir.ui.view">
            <field name="name">stock.warehouse.orderpoint.tree.editable.inherit</field>
            <field name="model">stock.warehouse.orderpoint</field>
            <field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree_editable"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="attributes">
                    <attribute name="string">Reordering Rules</attribute>
                </xpath>
                <xpath expr="//field[@name='qty_to_order']" position="after">
                    <field name="supplier_type"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
